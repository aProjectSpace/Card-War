rmdCards = [[], []]

def pickBetweenThreeNumber(plr):
    
def getNumber(plr):
    ran = r.randint(0,12)
    for i in rmdCards[plr]:
        if (i == ran):
            return getNumber()
    rmdCards[plr].append(ran)
    return ran
